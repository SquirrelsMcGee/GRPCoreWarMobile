    <string-array name="Instructions">
        <item>Instructions</item>
        <item>DAT-kills the task</item>
        <item>MOV-Moves instruction at A to B</item>
        <item>ADD-Adds A to B</item>
        <item>SUB-Subtracts A from B</item>
        <item>MUL-Multiplies A and B</item>
        <item>DIV-Divides B by A</item>
        <item>MOD-Modulus of B in A</item>
        <item>JMP-Jumps to A</item>
        <item>JMZ-If *B is 0, jumps to A</item>
        <item>JMN-If *B is not 0, jumps to A</item>
        <item>DJN-Decreases *B</item>
        <item>CMP-If *a == *B skips an instruction</item>
        <item>SPL-Creates a new task</item>
        <item>SEQ-Skips next instruction if A and B are equal</item>
        <item>SNE-Skips next instruction if A and B are not-equal</item>
        <item>SLT-Skips if *A less than *B</item>
        <item>XCH-Swaps A and B</item>
        <item>PCT-Protects operands from changing</item>
        <item>STP-Stores A into P-Space</item>
        <item>LDP-Retrieves data from P-Space</item>
    </string-array>
    <string-array name="Special">
        <item>Special Instructions</item>
        <item>END-Stops Compilation</item>
        <item>ORG Identifies start location</item>
        <item>PIN-Specifies P space identifier</item>
        <item>EQU-Replaces all instances</item>
    </string-array>
